  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Generate Ast",
      "request": "launch",
      "mainClass": "com.ab.tools.GenerateAst",
      "projectName": "lou"
    },
    {
      "type": "java",
      "name": "Launch REPL",
      "request": "launch",
      "mainClass": "com.ab.lou.Lou",
      "projectName": "lou"
    },
    {
      "type": "java",
      "name": "Execute Source File",
      "request": "launch",
      "mainClass": "com.ab.lou.Lou",
      "projectName": "lou",
      "args": "samples/hello.lou"
    },
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    }
  ]
}
